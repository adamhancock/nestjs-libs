{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@adamhancock/nestjs-eventhubs/(.*)": "<rootDir>/../libs/nestjs-eventhubs/src/$1",
    "@adamhancock/nestjs-eventhubs": "<rootDir>/../libs/nestjs-eventhubs/src"
  }
}